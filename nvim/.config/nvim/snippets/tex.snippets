#My snippets
snippet begin
	\begin{$1}
	$0
	\end{$1}

snippet thm
	\begin{$1}[$2]
	$0
	\end{$1}

snippet geq
	\geq 

snippet leq
	\leq 

snippet int
	\int_{$1}^{$2}

snippet frac
	\frac{$1}{$2}

snippet neq
	\begin{equation}
	$1
	\end{equation}

snippet eq
	\[
	$1
	\]

snippet ieq
	\( $1 \)

snippet cal "Math Cal"
	\mathcal{$1}$0
